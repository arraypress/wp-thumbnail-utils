{
  "name": "arraypress/wp-thumbnail-utils",
  "version": "1.0.0",
  "description": "A collection of WordPress utility functions for handling thumbnails, avatars, and image operations",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-thumbnail-utils/",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "files": [
      "src/attachment.php",
      "src/functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-thumbnail-utils/issues",
    "source": "https://github.com/arraypress/wp-thumbnail-utils"
  },
  "keywords": [
    "wordpress",
    "thumbnails",
    "images",
    "avatars",
    "media",
    "attachments",
    "user-images",
    "term-images",
    "post-thumbnails",
    "image-handling",
    "wp-media",
    "media-library"
  ]
}